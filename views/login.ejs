<%- include('partials/header') %> 

<section class="form" >
    <div id="container">
        <div class="user signInBox">
            <div class="imgBox">
                <img src="/images/gastronomic-translation-1.jpg" alt="Random image">
            </div>
            <div class="formBx">
                <form>
                    <h2>Sign In</h2>
                    <input type="text" name="" placeholder="Username">
                    <input type="password" name="" placeholder="Password">
                    <input type="submit" value="Login" name="">
                <p class="signup"> Don't have an account? <a href="#" class="swap">Sign Up</a> </p>
                </form>
                </div>
        </div>
        <div class="user signUpBox">
            <div class="formBx">
                <form>
                    <h2>Create an Account</h2>
                    <input type="text" name="" placeholder="Username">
                    <input type="email" name="" placeholder="Email Address">
                    <input type="password" name="" placeholder=" Create Password">
                    <input type="password" name="" placeholder="Confirm Password">
                    <input type="submit" value="Sign Up" name="">
                    <p class="signup"> Already have an account? <a href="#" class="swap">Sign In</a> </p>
                </form>
                </div>
                <div class="imgBox">
                    <img src="/images/gastronomic-translation-1.jpg" alt="Random image">
                </div>
        </div>
    </div>
</section>

<%- include('partials/footer') %> 

